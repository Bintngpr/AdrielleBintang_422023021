                            },
                            "example": {
                                "name": "Gabriel Tama",
                                "email": "gabrieltama@gmail.com",
                                "password": "gabri123",
                                "password_confirmation": "gabri123"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Invalid input",
